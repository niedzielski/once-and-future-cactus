{
  "name": "once-and-future-cactus",
  "version": "0.2.1",
  "description": "Once and Future Cactus",
  "keywords": [
    "game",
    "pixel",
    "8-bit",
    "2d",
    "phaser",
    "responsive"
  ],
  "homepage": "https://github.com/rndmem/once-and-future-cactus",
  "repository": "github:rndmem/once-and-future-cactus",
  "bugs": "https://github.com/rndmem/once-and-future-cactus/issues",
  "main": "src",
  "scripts": {
    "watch": "live-server",
    "build": "bin/make",
    "test": "npm run -s lint",
    "rebuild": "npm run -s clean && npm run -s build",
    "clean": "bin/make clean",
    "lint": "rndlint .",
    "update": "ncu -atu && jspm update",
    "postinstall": "jspm i",
    "preversion": "[ -z \"$(git status --porcelain)\" ] && npm run -s rebuild && npm run -s test && git add build",
    "postversion": "git push origin master --follow-tags && npm publish",
    "clobber": "npm run -s clean && rm -rf node_modules jspm_packages .eslintcache"
  },
  "engines": {
    "node": "7.1.0"
  },
  "dependencies": {
    "phaser": "2.6.2"
  },
  "devDependencies": {
    "eslint": "3.10.2",
    "eslint-config-rndmem": "3.0.0",
    "eslint-plugin-jsdoc": "2.3.1",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-node": "3.0.4",
    "eslint-plugin-promise": "3.4.0",
    "jspm": "0.17.0-beta.32",
    "live-server": "1.1.0",
    "mem-font": "3.0.1",
    "npm-check-updates": "2.8.6",
    "pre-commit": "1.1.3"
  },
  "jspm": {
    "dependencies": {},
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@0.0.17"
    }
  },
  "precommit.silent": true,
  "license": "GPL-3.0"
}
